<div class="container-fluid content-md">
	<!--=== Illustration v1 ===-->
	<div class="row margin-bottom-60">
		<div class="col-md-6 md-margin-bottom-30">
			<div class="overflow-h">
				<div class="illustration-v1 illustration-img1">
					<div class="illustration-bg">
						<div class="illustration-ads ad-details-v1">
							<h3>
								NEW YEAR <strong>SALE</strong> 30% - 60% <strong>off</strong>
							</h3>
							<a class="btn-u btn-brd btn-brd-hover btn-u-light" href="#">Shop
								Now</a>
						</div>
					</div>
				</div>
			</div>
		</div>
		<div class="col-md-6">
			<div class="overflow-h">
				<a class="illustration-v1 illustration-img2" href="#"> <span
					class="illustration-bg"> <span
						class="illustration-ads ad-details-v2"> <span
							class="item-time">Spring 2023</span> <span class="item-name">Simple</span>
					</span>
				</span>
				</a>
			</div>
		</div>
	</div>
	<!--/end row-->
	<!--=== End Illustration v1 ===-->

	<div class="heading heading-v1 margin-bottom-20">
		<h2>Highlight Products</h2>
		<p>A place where provide featured products from various shops.</p>
	</div>

	<!--=== Illustration v2 ===-->
	<div class="illustration-v2 margin-bottom-60">
		<div class="customNavigation margin-bottom-25">
			<a class="owl-btn prev rounded-x"><i class="fa fa-angle-left"></i></a>
			<a class="owl-btn next rounded-x"><i class="fa fa-angle-right"></i></a>
		</div>

		<ul class="list-inline owl-slider">

			<c:forEach items="${products}" var="p">
				<c:url value="/${not empty p.images?p.images:'images/video.png'}"
					var="imgUrl"></c:url>


				<%-- <td><c:if test="${p.images!=null}">
						<img width="50px" height="50px"
							src="<c:url value='/image?fname=products/${p.images}'/>">
					</c:if> <c:if test="${p.images==null}">
						<img width="50px" height="50px" id="images"
							src="<c:url value='/uploads/cate.png'/>">
					</c:if></td> --%>

				<c:url value="/${p.images!=null?p.images:'images/video.png'}"
					var="imgUrl"></c:url>
				<li class="item">
					<div class="product-img">
						<a
							href='<c:url value="/product/detail?productid=${p.productid}"/>'>
							<img width="300px" height="300px"
							src="<c:url value='/image?fname=products/${p.images}'/>" alt="">
						</a> <a class="product-review"
							href='<c:url value="/product/detail?productid=${p.productid}"/>'>Quick
							View</a> <a class="add-to-cart"
							
							href='<c:url value="/product/detail?productid=${p.productid}"/>'>
							<i class="fa fa-shopping-cart"></i>Add to cart
						</a>
						<div class="shop-rgba-red rgba-banner">Hightlight</div>
					</div>
					<div class="product-description product-description-brd">
						<div class="overflow-h margin-bottom-5">
							<div class="pull-left">
								<h5>
									<a
									
										href='<c:url value="/product/detail?productid=${p.productid}"/>'>${p.productname }</a>
								</h5>
								<h5>
									<span class="gender text-uppercase">${p.product_types.typename }</span>
								</h5>
								<%-- <h5><span class="gender">Đã xem:${p.views}</span></h5> --%>
							</div>
							<div class="product-price">
								<span class="title-price"></span> <span
									class="shop-red line-through"></span>

							</div>
						</div>
						<ul class="list-inline product-ratings">
							<li><i class="rating-selected fa fa-star"></i></li>
							<li><i class="rating-selected fa fa-star"></i></li>
							<li><i class="rating-selected fa fa-star"></i></li>
							<li><i class="rating fa fa-star"></i></li>
							<li><i class="rating fa fa-star"></i></li>
							<li class="like-icon"><a
								data-original-title="Add to wishlist" data-toggle="tooltip"
								<%-- data-placement="left" class="tooltips" href="${pageContext.request.contextPath }/likeVideo?videoId=${p.productid}&username=${sessionScope.acc.username}">${p.views} <i --%>
											class="fa fa-heart"></i></a></li>
							<li class="like-icon"><a data-original-title="Share"
								data-toggle="tooltip" data-placement="left" class="tooltips"
								href="${pageContext.request.contextPath }/shareVideo?videoId=${p.productid}">Share<i
									class="fa fa-heart"></i></a></li>
						</ul>
					</div>
				</li>
			</c:forEach>
		</ul>

	</div>
	<!--=== End Illustration v2 ===-->

	<div class="heading heading-v1 margin-bottom-20">
		<h2>CATEGORY</h2>
		<p>A place that provides categories by product</p>
	</div>
	<!--=== Illustration v2 ===-->
	<div class="illustration-v2 margin-bottom-60">


		<ul class="list-inline owl-slider-v2">
			<c:set var="temp" value="0"/>
			<c:forEach var="item" items="${types}">

				<c:url var="editURL" value="/product/list">
					<c:param name="typeid" value="${item.typeid}" />
				</c:url>
				<c:url value="${item.images}" var="imgUrl"></c:url>

				<li class="item">
					<div class="overflow-h margin-bottom-5">

						<a href="${editURL}&index1=-2"> <img src="${imgUrl}" alt=""></a>
						
						<div class="shop-rgba-red rgba-banner"> ${listCountProduct.get(temp)} product</div>
						<c:set var="temp" value="${temp+1}" />
						
						<div class="pull-center">
							<h5>
								<%-- <img src="${imgUrl}" alt="${item.typename}" class="type-image" /> --%>
								<img width="50px" height="50px" id="images"	src="<c:url value='/image?fname=types/${item.images}'/>">
								<a href='<c:url value="/findProductByTypeID?typeid=${item.typeid}"/>'><span class="gender text-uppercase">${item.typename}</span></a>
							</h5>

						</div>

					</div>
				</li>



			</c:forEach>
		</ul>


	</div>
	<!--=== End Illustration v2 ===-->


	

	
</div>
<!--=== End Product Content ===-->